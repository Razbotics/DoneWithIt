{
  "expo": {
    "name": "DoneWithIt",
    "slug": "DoneWithIt",
    "sdkVersion": "39.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#fc5c65"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#fc5c65"
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#fc5c65"
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "com.razbotics.donewithit",
      "versionCode": 1,
      "useNextNotificationsApi": true
    },
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": "An app for selling used items",
    "githubUrl": "https://github.com/Razbotics/DoneWithIt.git"
  }
}
